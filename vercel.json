{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/python",
            "config": {
                "distDir": "static"
            }
        },
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}